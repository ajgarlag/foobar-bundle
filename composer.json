{
    "name": "ajgarlag/foobar-bundle",
    "type": "symfony-bundle",
    "description": "Foobar bundle for Symfony applications",
    "keywords": ["foobar", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio J. Garc√≠a Lagar",
            "email": "aj@garcialagar.es"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "symfony/browser-kit": "^6.4|^7.3|^8.0",
        "symfony/css-selector": "^6.4|^7.3|^8.0",
        "symfony/framework-bundle": "^6.4|^7.3|^8.0",
        "symfony/security-bundle": "^6.4|^7.3|^8.0",
        "symfony/twig-bundle": "^6.4|^7.3|^8.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "nyholm/symfony-bundle-test": "^3.0",
        "symfony/phpunit-bridge": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": { "Ajgarlag\\Bundle\\FoobarBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajgarlag\\Bundle\\FoobarBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
